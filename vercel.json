{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "nextjs",
  "cleanUrls": false,
  "env": {
    "NEXT_PUBLIC_SOCKET_URL": "https://chitchat-socket-server.onrender.com",
    "NODE_VERSION": "18.x"
  },
  "build": {
    "env": {
      "NODE_VERSION": "18.x"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
