{
  "version": 2,
  "env": {
    "NEXT_PUBLIC_SOCKET_URL": "https://chitchat-socket-server.onrender.com",
    "NEXT_PUBLIC_APP_URL": "${VERCEL_URL}"
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
